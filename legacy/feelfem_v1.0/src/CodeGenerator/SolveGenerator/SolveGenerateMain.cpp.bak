#include "../../parser/Syntax/SNsolve.hpp"
#include "../../class/Solve.hpp"
#include "../../class/feelfuncs.hpp"


void GenerateSolve_feelp2skyline( Solve * );
void GenerateSolve_feelp2pcg( Solve *);
void GenerateSolve_feelp2achim( Solve * );
void GenerateSolve_feelp2achimcg( Solve * );
void GenerateSolve_feelp2iwashita( Solve * );
void GenerateSolve_feelp2achimloc( Solve * );
void GenerateSolve_feelfem90pamg( Solve * );
void GenerateSolve_feelfem90pamg81( Solve * );
void GenerateSolve_feelfem90samg20c( Solve * );
void GenerateSolve_feelfem90achimcg( Solve * );


void SolveGenerateMain( SNsolve *snSolvePtr )
{

  const Solve *solvePtr = snSolvePtr->getSolvePtr();

  cerr << "Now just return in CodeGenerator/SolveGenerator/SolveGenerateMain.cpp\n";
  return;

    

  solvePtr->MakeVariablePtrLst();   // Make solveVariablePtrLst from SE,D,N
  //  makeSL( solvePtr->GetLibraryType() );

  cerr << "NOW makeSL suspended in CodeGenerator/SolveGenerate/SolveGenerateMain.cpp\n";


  switch( solvePtr->GetLibraryType() ) {
    
  case TYPE_FEELP2SKYLINE:
    GenerateSolve_feelp2skyline( solvePtr );
    break;
    
  case TYPE_FEELP2PCG:
    GenerateSolve_feelp2pcg( solvePtr);
    break;

  case TYPE_FEELP2ACHIM:
    GenerateSolve_feelp2achim( solvePtr );
    break;

  case TYPE_FEELP2ACHIMCG:
    GenerateSolve_feelp2achimcg( solvePtr );
    break;

  case TYPE_FEELP2IWASHITA:
    GenerateSolve_feelp2iwashita( solvePtr );
    break;

  case TYPE_FEELP2ACHIMLOC:
    GenerateSolve_feelp2achimloc( solvePtr );
    break;

  case TYPE_FEELFEM90PAMG:
    GenerateSolve_feelfem90pamg( solvePtr );
    break;

  case TYPE_FEELFEM90PAMG81:
    GenerateSolve_feelfem90pamg81( solvePtr );
    break;

  case TYPE_FEELFEM90SAMG20c:
    GenerateSolve_feelfem90samg20c( solvePtr );
    break;

  case TYPE_FEELFEM90ACHIMCG:
    GenerateSolve_feelfem90achimcg( solvePtr );
    break;


  default:
    cerr << "Unknown solvePtr->GetLibraryType() = " <<
      solvePtr->GetLibraryType() << endl;
    abortExit(1);
    break;


  }

  return;
}
