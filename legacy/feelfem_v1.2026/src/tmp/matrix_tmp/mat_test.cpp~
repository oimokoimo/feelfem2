#include <stdio.h>
#include <iostream.h>

#include "matrix.hpp"
#include "string.hpp"

main()
{
  matrix <string> a(3,3);

  for(int i=0;i<3;i++) {
    for(int j=0;j<3;j++) {
      char buf[BUFSIZ];
      sprintf(buf," I am %d  %d",i+1,j+1);
      a.val(i,j) = buf;
    }
  }

  for(int i=0;i<3;i++) {
    for(int j=i;j<3;j++) {
      a.val(i,j) = a.val(j,i);
    }
  }

  for(int i=0;i<3;i++) {
    for(int j=0;j<3;j++) {
            cout << "(" << i << ","<<j<<")  "<< a.val(i,j) << endl;
    }
  }

  return(0);
}
