mesh {
  point a(0.1,0.1);
  point b(1,0.1);
  point c(1,1);
  point d(1,1.5);
  point e(0.5,1.5);
  point f(0.5,1);
  point g(0.3,1);
  point h(0.5,1.2);
  point i(0.3,1.2);
  point j(0.3,1);

  region aa[rect](a,b,c,f,j);
  region bb[rect](c,d,e,h,f);
  region cc[rect](f,h,i,j);


  domain x(a,b,c,d,e,f,g);

   freedom 600;	

}

var {
  fem p[p1];
  double a,b,c;

  const con = 100;

}

scheme {

oimo:
   p = a * x;
   b = c * b;
   goto oimo;
   goto kkk;
}

